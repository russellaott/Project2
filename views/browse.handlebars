<div id="browse-background">

  <div class="jumbotron jumbotron-fluid" id="browse-jumbo">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <h1 id="browse-title">See where people are going!</h1>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12" id="ver-header-2">
          <h4 class="browse-subtxt lead ml-4">Join a trip!</h4>
        </div>
      </div>
    </div>
  </div>

  <div class="container-fluid text-center" id="browse-nav">
    <div class="row">
      <div class="col-md-4">
        <button class="search-btn" id="home-btn">Go Home</button>
      </div>
      <div class="col-md-4">
        <button class="search-btn" id="host-btn">Host A Trip</button>
      </div>
      <div class="col-md-4">
        <button class="main-btn" id="search-btn">Search A Trip</button>
      </div>
    </div>
  </div>

  <div class="container">
    <div class="row">
      <div class="col-md-12 my-3" id="browseTripCard">
          {{#each trip}} {{>trips/trip-block}} {{/each}}
      </div>
    </div>
  </div>

  <script src="" async defer></script>
</div>